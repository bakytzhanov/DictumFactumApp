<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.jack.dictumfactum">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:parentActivityName=".StartActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".LoginActivity"
            android:parentActivityName=".StartActivity" />
        <activity android:name=".MessageActivity" />
        <activity
            android:name=".FeeActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".RawActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".FamilyActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".WorkActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PoliceActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".BorderActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".LandActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AdminActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ResetPasswordActivity"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".ProfileChangeActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".IntroActivity"
            android:theme="@style/Theme.Intro"></activity>
    </application>

</manifest>